{
  "name": "fe-conquest",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --configuration production",
    "start": "npm run env -s && ng serve --proxy-config proxy.conf.js",
    "deploy": "npm run env -s && ng deploy",
    "lint": "ng lint && stylelint \"src/**/*.scss\" && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --configuration=ci",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^19.0.5",
    "@angular/cdk": "^19.0.4",
    "@angular/common": "^19.0.5",
    "@angular/compiler": "^19.0.5",
    "@angular/core": "^19.0.5",
    "@angular/forms": "^19.0.5",
    "@angular/google-maps": "^19.0.4",
    "@angular/localize": "^19.0.5",
    "@angular/platform-browser": "^19.0.5",
    "@angular/platform-browser-dynamic": "^19.0.5",
    "@angular/platform-server": "^19.0.5",
    "@angular/router": "^19.0.5",
    "@auth0/angular-jwt": "^5.2.0",
    "@fortawesome/angular-fontawesome": "^1.0.0",
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@ng-select/ng-select": "^13.9.1",
    "@ngrx/effects": "^19.0.0",
    "@ngrx/store": "^19.0.0",
    "@ngx-translate/core": "^15.0.0",
    "@sentry/angular": "^10.31.0",
    "angulartics2": "^14.1.0",
    "bootstrap": "^5.3.3",
    "date-fns": "^3.6.0",
    "express": "^4.19.2",
    "ngx-sharebuttons": "^17.0.0",
    "rxjs": "^7.8.1",
    "swiper": "^12.0.3",
    "tslib": "^2.6.3",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular-eslint/builder": "^19.0.1",
    "@angular-eslint/eslint-plugin": "^19.0.1",
    "@angular-eslint/eslint-plugin-template": "^19.0.1",
    "@angular-eslint/schematics": "^19.0.1",
    "@angular-eslint/template-parser": "^19.0.1",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.5",
    "@angular/language-service": "^19.0.5",
    "@angularclass/hmr": "^3.0.0",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@ngx-rocket/scripts": "^4.0.0",
    "@popperjs/core": "^2.11.8",
    "@types/google.maps": "^3.58.1",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^20.14.2",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.57.0",
    "hads": "^2.0.1",
    "htmlhint": "^1.1.4",
    "https-proxy-agent": "^7.0.4",
    "jasmine-core": "^5.1.2",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "prettier": "^3.3.2",
    "stylelint": "^16.6.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-scss": "^6.3.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
